<ul class="pagination flex-wrap">
    {% if pagination.first_page %}
        <li class="page-item">
            <a class="page-link" href="{{ pagination.first_page.url }}/p1">1
                <span class="material-symbols-outlined">
                    keyboard_double_arrow_left
                </span>
            </a>
        </li>
    {% endif %}
    {% for pageNumber,page in pagination.pages %}
        <li class="page-item {{ page.active }}">
            <a class="page-link" href="{{ page.url }}">{{ pageNumber }}</a>
        </li>
    {% endfor %}
    {% if pagination.last_page %}
        <li class="page-item">
            <a class="page-link" href="{{ pagination.last_page.url }}/p{{ pagination.last_page.number }}">
                <span class="material-symbols-outlined">
                    keyboard_double_arrow_right
                </span>
                {{ pagination.last_page.number }}</a>
        </li>
    {% endif %}
</ul>